{
  "name": "mocha-exports-amd",
  "version": "1.1.1-in-progress.8",
  "description": "AMD loader plugin to run Mocha tests with module exports interface in browser",
  "main": "mochaExport.js",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "peerDependencies": {
    "mocha": ">=1.2.1"
  },
  "devDependencies": {
    "amdefine": "1.0.0",
    "mocha": "2.5.3",
    "requirejs": "2.2.0",
    "requirejs-domready": "2.0.3"
  },
  "scripts": {
    "test": "npm run test-regular && npm run test-amdefine-intercept",
    "test-regular": "mocha example/*.test.js",
    "test-amdefine-intercept": "mocha --require amdefine/intercept example/*.test.noShim.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/scottfreecode/mocha-exports-amd.git"
  },
  "keywords": [
    "Mocha",
    "testing",
    "AMD",
    "plugin",
    "loader-plugin",
    "AMD-loader-plugin",
    "AMD-plugin",
    "module",
    "exports",
    "exports-interface",
    "module-exports-interface"
  ],
  "author": "ScottFreeCode",
  "license": "AFL-3.0",
  "bugs": {
    "url": "https://github.com/scottfreecode/mocha-exports-amd/issues"
  },
  "homepage": "https://github.com/scottfreecode/mocha-exports-amd#readme"
}
